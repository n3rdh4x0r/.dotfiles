[global_config]
[keybindings]
[profiles]
  [[default]]
    scrollback_infinite = True
        custom_command = /home/$USER/.tmux_next_session
        exit_action = restart  # Optional: restart terminator if tmux exits
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
